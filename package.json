{
  "name": "chamel",
  "version": "0.1.44",
  "keywords": [
    "react",
    "react-component",
    "material design",
    "modern ui",
    "metro ui",
    "human ui",
    "native ui theme"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/skystebnicki/chamel.git"
  },
  "license": "MIT",
  "author": "Sky Stebnicki",
  "bugs": {
    "url": "https://github.com/skystebnicki/chamel/issues"
  },
  "homepage": "http://www.skystebnicki.com/",
  "scripts": {
    "pretest": "npm install",
    "build": "babel ./src --out-dir ./lib && npm run sass",
    "sass": "cpx './src/**/*.scss' ./lib",
    "test": "node node_modules/karma/bin/karma start test/karma.conf.js",
    "test-single-run": "node node_modules/karma/bin/karma start test/karma.conf.js  --single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor test/protractor-conf.js"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.21",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-dev-warning": "^0.1.0",
    "babel-plugin-transform-react-constant-elements": "^6.3.13",
    "babel-plugin-transform-react-inline-elements": "^6.3.13",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.2",
    "babel-plugin-transform-replace-object-assign": "^0.2.1",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-runtime": "^6.3.19",
    "babelify": "*",
    "classnames": "^1.2.0",
    "cpx": "^1.3.2",
    "css-loader": "^0.23.1",
    "karma": "~0.12.23",
    "karma-browserify": "^4.0.0",
    "karma-chrome-launcher": "^0.1.4",
    "karma-firefox-launcher": "^0.1.3",
    "karma-jasmine": "~0.2.0",
    "localforage": "^1.2.2",
    "material-ui": "^0.15.4",
    "node-sass": "^3.7.0",
    "object-hash": "^0.8.0",
    "process": "^0.11.2",
    "protractor": "~1.0.0",
    "react-tap-event-plugin": "^1.0.0",
    "sass-loader": "^4.0.0",
    "shelljs": "^0.2.6",
    "style-loader": "^0.13.1",
    "tmp": "0.0.23"
  },
  "description": "This is a ReactJS UI framework designed to provide the most native experience possible automatically across common platforms and devices.",
  "main": "lib/",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "stage-1",
            "react"
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "bootstrap-slider": "^5.1.0",
    "browserify": "*",
    "classnames": "^1.2.2",
    "codemirror": "^5.7.0",
    "fbjs": "^0.2.1",
    "localforage": "^1.2.10",
    "object-hash": "^0.8.0",
    "react": "^15.0.1",
    "react-addons-css-transition-group": "^15.0.1",
    "react-addons-transition-group": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-router": "^2.1.1",
    "react-tappable": "^0.8.3",
    "ua-parser-js": "^0.7.9"
  }
}
