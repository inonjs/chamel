{
  "name": "chamel",
  "version": "0.2.31",
  "keywords": [
    "react",
    "react-component",
    "material design",
    "modern ui",
    "metro ui",
    "human ui",
    "native ui theme"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/skystebnicki/chamel.git"
  },
  "license": "MIT",
  "author": "Sky Stebnicki",
  "bugs": {
    "url": "https://github.com/skystebnicki/chamel/issues"
  },
  "homepage": "http://www.skystebnicki.com/",
  "scripts": {
    "pretest": "npm install",
    "build": "babel ./src --out-dir ./lib && npm run sass",
    "sass": "cpx './src/**/*.scss' ./lib",
    "test": "NODE_ENV=test jest --watch --coverage",
    "test-single-run": "NODE_ENV=test jest --coverage",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor test/protractor-conf.js"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.21",
    "babel-eslint": "^6.0.0",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-module-resolver": "^2.7.0",
    "babel-plugin-transform-dev-warning": "^0.1.0",
    "babel-plugin-transform-react-constant-elements": "^6.3.13",
    "babel-plugin-transform-react-inline-elements": "^6.3.13",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.2",
    "babel-plugin-transform-replace-object-assign": "^0.2.1",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-runtime": "^6.3.19",
    "classnames": "^1.2.0",
    "cpx": "^1.3.2",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "jest": "*",
    "jest-css-modules": "^1.1.0",
    "jest-junit": "^1.5.1",
    "localforage": "^1.2.2",
    "node-sass": "^3.7.0",
    "object-hash": "^0.8.0",
    "process": "^0.11.2",
    "protractor": "~1.0.0",
    "react-tap-event-plugin": "^1.0.0",
    "react-test-renderer": "^15.5.4",
    "sass-loader": "^4.0.0",
    "shelljs": "^0.2.6",
    "style-loader": "^0.13.1",
    "tmp": "0.0.23",
    "transfer-webpack-plugin": "^0.1.4",
    "webpack": "^1.13.2"
  },
  "description": "This is a ReactJS UI framework designed to provide the most native experience possible automatically across common platforms and devices.",
  "main": "lib/",
  "dependencies": {
    "bootstrap-slider": "^5.1.0",
    "browserify": "*",
    "classnames": "^1.2.2",
    "codemirror": "^5.7.0",
    "draft-js": "^0.10.1",
    "draft-js-export-html": "^0.5.0",
    "draft-js-import-html": "^0.3.0",
    "fbjs": "^0.2.1",
    "immutable": "^3.8.1",
    "localforage": "^1.2.10",
    "object-hash": "^0.8.0",
    "prop-types": "^15.5.9",
    "react": "^15.0.1",
    "react-addons-css-transition-group": "^15.0.1",
    "react-addons-transition-group": "^15.0.1",
    "react-codemirror": "^0.2.6",
    "react-dom": "^15.0.1",
    "react-draggable2": "^0.7.0-alpha1",
    "react-router": "^2.1.1",
    "ua-parser-js": "^0.7.9"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/vendor/",
      "/build/",
      "/sass/"
    ],
    "testResultsProcessor": "./node_modules/jest-junit",
    "coverageDirectory": "./test/reports/coverage"
  },
  "jest-junit": {
    "suiteName": "chamel",
    "output": "./test/reports/junit.xml"
  }
}
